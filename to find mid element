import java.util.*;
class HelloWorld {
    public static void main(String[] args) {
        LinkedList<Integer> l=new LinkedList<>();
        l.add(1);
        l.add(2);
        l.add(3);
        l.add(4);
        l.add(5);
        System.out.println(l);
        int size=l.size();
        int mid=size/2;
        System.out.println("The middle value is: "+l.get(mid));
        
    }
}
